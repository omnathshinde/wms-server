{
	"name": "server",
	"type": "module",
	"license": "ISC",
	"version": "1.0.0",
	"main": "src/server.js",
	"author": "Omnath Shinde",
	"description": "Node.js Server",
	"keywords": [
		"Node.js",
		"Express"
	],
	"imports": {
		"#/*": "./*",
		"#logs": "./logs",
		"#src/*": "./src/*",
		"#app/*": "./src/app/*",
		"#db/*": "./src/app/database/*",
		"#routes/*": "./src/routes/*",
		"#controllers/*": "./src/controllers/*"
	},
	"scripts": {
		"start": "node src/server",
		"dev": "nodemon src/server",
		"prepare": "husky",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check ."
	},
	"dependencies": {
		"argon2": "^0.44.0",
		"compression": "^1.8.1",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"dayjs": "^1.11.18",
		"dotenv": "^17.2.3",
		"express": "^5.1.0",
		"express-async-handler": "^1.2.0",
		"express-rate-limit": "^8.1.0",
		"helmet": "^8.1.0",
		"http-errors": "^2.0.0",
		"jsonwebtoken": "^9.0.2",
		"morgan": "^1.10.1",
		"multer": "^2.0.2",
		"mysql2": "^3.15.3",
		"sequelize": "^6.37.7",
		"winston": "^3.18.3"
	},
	"devDependencies": {
		"@eslint/js": "^9.36.0",
		"eslint": "^9.36.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^5.5.4",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"globals": "^16.4.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.6",
		"nodemon": "^3.1.10",
		"prettier": "^3.6.2",
		"prettier-eslint": "^16.4.2"
	},
	"lint-staged": {
		"**/*.{js}": [
			"eslint --fix",
			"prettier --write"
		],
		"**/*.{json,md,css,html}": [
			"prettier --write"
		]
	}
}
